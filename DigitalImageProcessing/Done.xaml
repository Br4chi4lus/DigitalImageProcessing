<Window x:Class="DigitalImageProcessing.Done"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigitalImageProcessing"
        mc:Ignorable="d"
        Title="Done" Height="140" Width="300">
    <Grid>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Top">File has been processed</TextBlock>
        <TextBlock Name="time" Text="" Margin="0,20,0,40" HorizontalAlignment="Center" VerticalAlignment="Top"></TextBlock>
        <Button Name="ok" Width="60" Height="20" Click="ok_Click" Margin="0,50,0,0">OK</Button>
    </Grid>
</Window>
